{
    "name": "genese",
    "version": "1.0.1",
    "scripts": {
        "build": "tsc && cp -r src/complexity/json-ast-to-reports/templates dist/src/complexity/json-ast-to-reports/templates && cp -r src/complexity/automatic-refactoring/templates dist/src/complexity/automatic-refactoring/templates && cp -r src/complexity/core/chartjs dist/src/complexity/core/chartjs",
        "dev": "npm run build && node dev.js",
        "replace": "npm run build && npm unpublish genese@0.2.9 --force && npm publish && npm i -g genese"
    },
    "author": {
        "name": "Gilles Fabre",
        "url": "http://gillesfabre.com/"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/geneseframework/genese"
    },
    "bin": {
        "genese": "dist/index.js"
    },
    "dependencies": {
        "@types/node": "^13.11.1",
        "ansi-colors": "^4.1.1",
        "chalk": "^4.0.0",
        "chart.js": "^2.9.3",
        "commander": "^5.0.0",
        "eol": "^0.9.1",
        "fs-extra": "^9.0.0",
        "genese-mapper": "^1.0.6",
        "handlebars": "^4.7.6",
        "java-ast": "^0.3.0",
        "java-parser": "0.6.0",
        "paths": "^0.1.1",
        "prismjs": "^1.20.0",
        "terminal-link": "^2.1.1",
        "ts-morph": "^7.1.2",
        "ts-node": "^9.0.0"
    },
    "devDependencies": {},
    "jscpd": {
        "threshold": 3,
        "reporters": [
            "html",
            "console"
        ],
        "ignore": [],
        "absolute": true,
        "gitignore": true
    }
}
